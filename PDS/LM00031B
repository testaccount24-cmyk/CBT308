++ USERMOD (LM00031)        REWORK(2001005)
       /* MODIFY CONSOLE MSG IEE163I MODE= ...
          THIS VERSION SUPPORTS A 4 DIGIT UCB ADDRESS FOR SYSRES
       */ .
++ VER (Z038) FMID(JBB7713) .
++ ZAP (IEECVETD) .
* MOD TO CHANGE CONSOLE IEE163I MODE= MESSAGE FOR NEAT INFO
*
* BECAUSE OF THE WAY BASE REGISTER IS SET UP, BRANCH TARGET ADDRESSES
* ARE 2 BYTES LESS THAN THE ACTUAL ADDRESS
*
NAME IEECVET1 IEECVETD
 EXPAND IEECVETD(64)
VER 0316 9108,C02A
VER 0548 5C5C,40D7                  ** PATCH AREA
*
REP 0316 47F0,A546           <---   B     PATCH AREA
REP 0548 41F0,A550           <---   LA    R15,*+10     31 BIT ADDR
REP 054C 56F0,A596           <---   O     R15,=A(X'80000000')
REP 0550 0B0F                       BSM   R0,R15
REP 0552 58E0,0010                  L     R14,16
REP 0556 58F0,E030                  L     R15,48(,R14)
REP 055A 9260,2010                  MVI   16(R2),C'-'
REP 055E D205,2011,F01C             MVC   17(6,R2),28(R15) SYSRES
REP 0564 9261,2017                  MVI   23(R2),C'/'
REP 0568 F342,2018,F004             UNPK  24(5,R2),4(3,R15)
REP 056E 41F0,A572           <---   LA    R15,*+6          24 BIT MODE
REP 0572 0B0F                       BSM   R0,R15
REP 0574 D403,2018,A5AA      <---   NC    24(4,R2),=X'0F0F0F0F'
REP 057A DC03,2018,A59A      <---   TR    24(4,R2),=C'0123456789ABCDEF'
REP 0580 9260,201C                  MVI   28(R2),C'-'
REP 0584 41F0,0028                  LA    R15,40
REP 0588 1BEF                       SR    R14,R15
REP 058A D207,201D,E000             MVC   29(8,R2),0(R14)  VERSION
REP 0590 9108,C02A                  TM    42(,R12),X'08'
REP 0594 47F0,A318           <---   B     792(,R10)
REP 0598 8000,0000                  =A(X'80000000')
REP 059C F0F1,F2F3,F4F5,F6F7        =C'01234567'
REP 05A4 F8F9,C1C2,C3C4,C5C6        =C'89ABCDEF'
REP 05AC 0F0F,0F0F                  =X'0F0F0F0F'

